package com.index;

import java.util.Scanner;
public class OddorEven {

	public static void main(String[] args) {
		Scanner obj = new Scanner(System.in);
		
		System.out.print("Enter a number");
		int num = obj.nextInt();
		
		if(num%2==0) {
			System.out.println("Even");
		}else {
			System.out.println("Odd");
		}
	}

}
